pipeline{
    agent{
        label 'java-slave'
    }
    stages{
        stage('Build') {
            steps{
                echo "This is a build stage"
            }
        }
        stage('groovycodestage'){
            steps {
                script {
                    def course = "k8s"]

                    if(course == "k8s")
                    println("thanks for enrolling to k8s course")
                    else 
                    println("Do Enroll to K8S")
                }
            }
        }
    }

}
